{
  "name": "coldoutreach",
  "version": "0.1.0",
  "description": "",
  "homepage": ".",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/gitcoinco/coldoutreach.git"
  },
  "dependencies": {
    "antd": "^3.2.3",
    "dotenv": "^5.0.1",
    "history": "^4.7.2",
    "node-sass-chokidar": "1.1.0",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-lz-editor": "^0.11.9",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^1.1.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.18.0",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.18.2",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.0.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "proxy": {
    "/profile": {
      "changeOrigin": true,
      "target": "https://coldoutreach-staging.herokuapp.com/"
    }
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/styles/scss -o src/assets/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/styles/scss/ -o src/assets/styles/css/ --watch --recursive",
    "start": "concurrently --kill-others \"react-scripts start\" \"npm run watch-css\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
